terraform {
  required_version = ">= 1.3.0"
  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "~> 5.0"
    }

    archive = {
      source  = "hashicorp/archive"
      version = "~> 2.4.0" # You can adjust this version as needed
    }
  }
}

# Configure the AWS Provider
provider "aws" {}
